{
  "expo": {
    "name": "Raksha - Personal Safety",
    "slug": "raksha-safety-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "raksha",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "expo-location"],
    "experiments": {
      "typedRoutes": true
    },
    "permissions": ["LOCATION", "CAMERA", "MICROPHONE"]
  }
}